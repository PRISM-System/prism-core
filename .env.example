    # Hugging Face (required for vLLM model downloads)
    HUGGING_FACE_TOKEN=YOUR_HF_TOKEN

    # vLLM (OpenAI-Compatible server)
    VLLM_MODEL=Qwen/Qwen3-8B
    VLLM_HOST=0.0.0.0
    VLLM_PORT=8001
    # Additional vLLM args (e.g., tool calling parsers). Leave as-is or customize.
    VLLM_ARGS=--enable-auto-tool-choice --tool-call-parser hermes

    # PRISM-Core â†” vLLM (OpenAI-Compatible) integration
    # PrismLLMService uses this base URL to call the vLLM server
    VLLM_OPENAI_BASE_URL=http://vllm:${VLLM_PORT}/v1
    # vLLM default accepts any key; keep EMPTY unless server enforces auth
    OPENAI_API_KEY=EMPTY

    # Database (PostgreSQL)
    POSTGRES_USER=myuser
    POSTGRES_PASSWORD=mysecretpassword
    POSTGRES_DB=mydatabase
    DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}